#example of defining a menu using messages.properties.

#menus.X namespace defines entries for menu with key "X" where "default" is the default value.
#menus.X.list= list of top level menu/tool names
menus.default.list=one,two,three

#menus.X.title.Y = display title of menu Y
# if this is not present then "Y" will be the title
menus.default.title.one=One
menus.default.title.two=Two
menus.default.title.three=Three
menus.default.title.x=Add
menus.default.title.y=Delete

#menus.X.image.Y = path of an image for menuitem Y

menus.default.image.one=skin/house.png
menus.default.image.two=skin/information.png
menus.default.image.three=skin/database_edit.png
menus.default.image.x=skin/database_save.png
menus.default.image.y=skin/database_add.png


#menus.X.link.Y = path to link for menu item, in the form "controller:action" or just "action" for the current controller.
menus.default.link.one=monkey:menuSelect?name=one
menus.default.link.two=monkey:menuSelect?name=two
menus.default.link.three=test:index?name=three
menus.default.link.x=monkey:menuSelect?name=one&subitem=x
menus.default.link.y=monkey:menuSelect?name=one&subitem=y

#menus.X.content.Y= list of second level menu/tool names for menu Y
#the list values are menu names.  Define the menu in the same way as for X
menus.default.content.one=x,y
menus.default.content.two=two,three
menus.default.content.three=three,one
menus.default.content.monkey=x,y,one,two
